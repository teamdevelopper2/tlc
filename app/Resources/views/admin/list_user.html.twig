<h1>Liste des utilisateur</h1>

<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Status</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>{{ user.username }}</tr>
        <tr>{{ user.email }}</tr>
        <tr>{{ user.enabled }}</tr>
        <tr>
            {% for profil in user.roles %}
                {{ profil.role }} |
            {% endfor %}
        </tr>
    {% endfor %}
    </tbody>
</table>